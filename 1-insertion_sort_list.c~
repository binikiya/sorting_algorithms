#include "sort.h"

/**
 * insertion_sort_list - sort the list using insertion sort
 * @list: pointer of lists that have integer
 */
void insertion_sort_list(listint_t **list)
{
  listint_t *temp = NULL;
  listint_t *current = NULL, *prev = NULL;
  if (!list)
    return;
  temp = *list;
  while (temp)
    {
      prev = temp->prev;
      while (prev)
	{
	  if (temp->n >= prev->n)
	    break;
	  swapped(current, list);
	  current = prev;
	  prev = current;
	  
